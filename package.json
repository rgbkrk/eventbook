  },
  "napi": {
    "name": "eventbook",
    "targets": ["wasm32-wasip1-threads"]
  },
  "dependencies": {
    "@napi-rs/wasm-runtime": "^0.2.4"
  },
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0"
